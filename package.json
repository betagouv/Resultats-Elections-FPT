{
  "name": "resultats-elections-fpt",
  "version": "1.0.0",
  "description": "Saisir et centraliser les résultats des élections professionnelles dans la fonction publique territoriale",
  "type": "module",
  "scripts": {
    "dev:serve-html": "http-server --port 5173 ./apps-html",
    "dev:fiche": "vite --mode vue-fiche",
    "dev:tableau": "vite --mode vue-tableau",
    "dev:recherche": "vite --mode vue-recherche",
    "build:fiche": "vite build --mode vue-fiche",
    "build:tableau": "vite build --mode vue-tableau",
    "build:recherche": "vite build --mode vue-recherche",
    "build": "npm-run-all --parallel build:fiche build:tableau build:recherche",
    "preview:fiche": "vite preview --mode vue-fiche",
    "preview:tableau": "vite preview --mode vue-tableau",
    "preview:recherche": "vite preview --mode vue-recherche"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/Resultats-Elections-FPT.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/betagouv/Resultats-Elections-FPT/issues"
  },
  "homepage": "https://github.com/betagouv/Resultats-Elections-FPT#readme",
  "dependencies": {
    "@gouvfr/dsfr": "^1.14.3",
    "@gouvminint/vue-dsfr": "^8.13.1",
    "http-server": "^14.1.1",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "npm-run-all": "^4.1.5",
    "vite": "^5.2.0"
  }
}
